<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.superfit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_icono_app_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Antropometria" />
        <activity android:name=".Mensualidad" />
        <activity android:name=".Cuestionario" />
        <activity android:name=".Registro" />
        <activity android:name=".EditarPerfil" />
        <activity android:name=".Detallejercicio" />
        <activity android:name=".Sets" />
        <activity android:name=".Ejercicios" />
        <activity android:name=".Rutinas" />
        <activity android:name=".Perfil" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".Inicio"
                android:theme="@style/splashtheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>